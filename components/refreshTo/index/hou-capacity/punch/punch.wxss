page {
    background-color: rgb(241, 237, 237);
}
/* 总的打卡情况 */
.allPunch {
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-top: 30rpx;
}
/* 每个楼层的情况 */
.floorCondition {
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    height: 200rpx;
    width: 95%;
    background-color: #ffffff ;
    padding: 0 35rpx 0 35rpx;
    font-size: 40rpx;
    font-weight: 550;
    border-radius: 25rpx;
    box-shadow: 0 2rpx 0 0 #cecece
}
.dormCount {
    display: flex;
    justify-content: space-between;
    font-size: 30rpx;
    font-weight: 500;
}
